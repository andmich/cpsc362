@model PasswordManagerProject.Models.ProfileViewModel

<div class="form-group">
    <label class="col-sm-3 control-label">Password</label>
    <div class="col-sm-6">
        @Model.PasswordInfo.Password
    </div>
</div>
<div class="clearfix"></div>
<br />
<div class="form-group">
    <label class="col-sm-3 control-label">Username for Password</label>
    <div class="col-sm-6">
        @Model.PasswordInfo.Username
    </div>
</div>
<div class="clearfix"></div>
<br />
<div class="form-group">
    <label class="col-sm-3 control-label">Security Answer</label>
    <div class="col-sm-6">
        @Model.PasswordInfo.SecurityAnswer
    </div>
</div>
<br />
<div class="clearfix"></div>
<div class="form-group">
    <label class="col-sm-3 control-label">Label</label>
    <div class="col-sm-6">
        @Model.PasswordInfo.LabelType
    </div>
</div>
